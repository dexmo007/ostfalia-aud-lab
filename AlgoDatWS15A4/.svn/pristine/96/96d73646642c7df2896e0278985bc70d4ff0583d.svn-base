package de.ostfalia.aud.ws15.comparators;

import de.ostfalia.aud.ws15.Contact;

import java.util.Comparator;

/**
 * Created by Henrik on 10/30/2015.
 * @author Henrik and Maxi
 */
public class NameOnlyComparator implements Comparator<Contact> {
    /**
     * compares only the names of the contacts
     *
     * @param o1 first contact
     * @param o2 second contact
     *
     * @return string compareTo value of the two names
     */
    @Override
    public int compare(Contact o1, Contact o2) {
        return o1.getName().compareTo(o2.getName());
    }


}
